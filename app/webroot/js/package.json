    "browserify": "^14.3.0",
    "uglify": "^0.1.5",
    "watchify": "^3.9.0"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify -t [ babelify --presets [ es2015 ] ] misp.es6.js -o misp.js",
